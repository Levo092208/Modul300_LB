# Box-Einstellungen
Vagrant . configure ( "2" )  do | config |
  config . vm . box  =  "ubuntu / trusty64"

# Provider-Einstellungen
config  .  vm  .  Angebote   "virtualbox"   do | vb |
  vb  .  name   =   "VM-M300-LB02"
  vb  .  gui   =   wahr
  vb  .  Speicher   =   "4096"

end